# -*- coding:utf-8 -*-
development:
  version: "v1.0.0.b"
  activeresource:
    lgdpf:
      site: "http://localhost:3000"
      user: 
      password: 
      proxy: 
    lgdis:
      site: "http://localhost:3003"
      user: 
      password: 
      proxy: 
      api_key: 
      prefix: 
  ldap_auth:
    enable: false
  external_auth:
    enable: true
  # sheltersテーブルの同期元情報
  shelter_source:
    uri: http://localhost:3000/exportdata/
    timeout: 60
    basic_auth:
      username: 
      password: 
  # SMTPサーバの設定
  mail:
    address: smtp.example.com
    port: 587
    authentication: :plain
    user_name: username
    password: password
    domain: smtp.example.com
    enable_starttls_auto: true
    sender: saitai@example.com
    host: localhost:3000

